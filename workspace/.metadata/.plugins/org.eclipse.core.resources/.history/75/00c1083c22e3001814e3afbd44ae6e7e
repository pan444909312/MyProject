package com.leetcode.pjx;

import java.math.*;
public class Subject007 {
    public int reverse(int x) {
    	
    	int[] a = {1,2,3,4,5,6} ;

    	for(int i=0;x>0;i++){
    		if(x<10){
    			a[i] = x;
    		}
    		else{
    			a[i] = x%10;
    		}
    		x = x/10;
    	}
    	
    	for(int i=0;i<a.length;i++){
    		System.out.println(a[i]);
    	}
    	
        return 0;
    }
    public static void main(String[] args) {
    	Subject007 s = new Subject007();
    	s.reverse(123);
	}
}
