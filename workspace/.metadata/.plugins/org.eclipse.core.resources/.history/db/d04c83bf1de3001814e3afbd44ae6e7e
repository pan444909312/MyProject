package com.leetcode.pjx;

import java.math.*;
public class Subject007 {
    public int reverse(int x) {
    	
    	String a[];
    	String b[];
    	int j;
    	for(int i=0;x>0;i++){
    		a[i] = x - (x/(int)Math.pow(10, i+1))*(int)Math.pow(10, i+1);
    		
    	}
    	
        return 0;
    }
    
}
