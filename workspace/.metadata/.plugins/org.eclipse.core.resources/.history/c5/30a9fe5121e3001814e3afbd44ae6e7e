package com.leetcode.pjx;

import java.math.*;
public class Subject007 {
    public int reverse(int x) {
    	
    	int[] a = null ;

    	for(int i=0;x>0;i++){
    		if(x<10){
    			a[i] = x;
    		}
    		else{
    			a[i] = x%10;
    		}
    		x = x/10;
    	}
    	
    	for(int i=0;i<a.length;i++){
    		System.out.println(a[i]);
    	}
    	
        return 0;
    }
    
}
